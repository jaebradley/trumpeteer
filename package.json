{
  "name": "trumpeter",
  "version": "0.0.0-development",
  "description": "Trump tweets in your command line",
  "main": "index.js",
  "bin": {
    "trumpeter": "build/executables/trumpeter.js"
  },
  "scripts": {
    "codecov": "codecov",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "compile": "babel -d build/ src/ --ignore node_modules,*.test.js",
    "lint": "eslint --ext .js .",
    "test": "jest --coverage",
    "prepublishOnly": "npm run compile",
    "semantic-commit": "commit",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaebradley/trumpeter.git"
  },
  "keywords": [
    "trump",
    "cli",
    "node"
  ],
  "author": "jae.b.bradley@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaebradley/trumpeter/issues"
  },
  "homepage": "https://github.com/jaebradley/trumpeter#readme",
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@commitlint/cli": "^5.1.1",
    "@commitlint/config-angular": "^5.1.1",
    "@commitlint/prompt": "^5.1.2",
    "@commitlint/prompt-cli": "^5.1.2",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "codecov": "^3.0.0",
    "eslint": "^4.12.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "semantic-release": "^8.2.0"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "commander": "^2.12.1",
    "inquirer": "^4.0.0",
    "keytar": "^4.0.5",
    "moment-timezone": "^0.5.14",
    "say": "^0.14.0",
    "twitter": "^1.7.1"
  }
}
